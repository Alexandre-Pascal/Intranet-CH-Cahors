    "prisma": "^5.12.1",
    "react": "^18",
    "react-dom": "^18",
    "react-quill": "^2.0.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "nodemon": "^3.1.0",
    "postcss": "^8",
